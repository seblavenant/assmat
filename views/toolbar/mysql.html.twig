{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {# Symfony Logo #}
    {% set icon %}
        <img alt="Symfony" src="data:image/png;base64,
" />
<span class="sf-toolbar-status">{{ collector.data | length }}</span>
    {% endset %}
    {% set text %}
        <b>Requêtes SQL</b> : {{ collector.data | length }}
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img src="data:image/png;base64,
" alt="Mysql"></span>
    <b>Requêtes SQL</b>
</span>
{% endblock %}

{% block panel %}
    <h2>Requêtes SQL</h2>
    <table>
       <tr>
           <th>Query</th>
       </tr>
       {% for query in collector.data%}
           <tr>
               <td>{{ query }}</td>
           </tr>
       {% endfor %}
   </table>
{% endblock %}
